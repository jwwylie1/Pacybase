  <%- include ('public/partials/header.ejs') %>

    <!--FILTER BOX-->
    <div id="filterBox">
        <div class="deletesearchmsg">FIND YOUR POST</div>
        <input type="text" id="searchBarDelete" class="deleteSearchBar" placeholder="Enter your Reddit username">
        <br/>
        <!--filter button-->
        <button id="deletesearchbtn" onclick="searchDelete()">SEARCH</button>
    </div>

