                        <div class="cardbar">
                          <div class="overall"><%=cardsList[trades[i].has1].ovr%></div>
                          <div class="position"><%=cardsList[trades[i].has1].pos%></div>
                          <div class="cardflagctr">
                              <img loading="lazy" class="cardflag" src="<%= `/files/imgs/flags/${cardsList[trades[i].has1].nation}.png` %>"/>
                          </div>
                          <div class="cardbadgectr">
                              <img loading="lazy" class="cardbadge" src="<%= `/files/imgs/clubs/${cardsList[trades[i].has1].lg}/${cardsList[trades[i].has1].club}.png` %>"/>
                          </div>
                        </div>
