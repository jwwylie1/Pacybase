<div id="sbccards">
    <input type="text" id="sbcsearchbar" onkeyup="sbcSearchFilter()" placeholder="Search for names.." autocomplete="off">

    <div class="filterbar">
        <div class="filterelement" id="posfilterctr">
            <div class="filtercircle" id="poscircle" onclick="showPosFilter()"></div>
